{
  "server": {
    "host": "0.0.0.0",
    "port": 8080,
    "workers": 4,
    "tcp_backlog": 1024,
    "batch_size_limit": 100
  },
  "cache": {
    "enabled": true,
    "memory": {
      "max_size": 100000
    }
  },
  "logging": {
    "level": "info",
    "format": "json",
    "output": "stdout"
  },
  "telemetry": {
    "enabled": false
  },
  "prometheus": {
    "enabled": false
  },
  "limits": {
    "accounts_blacklist": []
  },
  "upstreams": {
    "steemd": [
      ["https://api.steem.fans", 1, 30],
      ["https://api.justyy.com", 1, 30]
    ],
    "appbase": {
      "condenser_api": [
        ["https://api.steem.fans", 1, 30],
        ["https://api.justyy.com", 1, 30]
      ],
      "database_api": [
        ["https://api.steem.fans", 3, 30],
        ["https://api.justyy.com", 3, 30]
      ]
    }
  }
}

